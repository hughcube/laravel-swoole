{
  "name": "hughcube/laravel-swoole",
  "description": "The Laravel Migration package.",
  "license": "MIT",
  "authors": [
    {
      "name": "hugh.li",
      "email": "hugh.li@foxmail.com"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpstan/phpstan": "^0.9.0|^0.11.0|^0.12.0",
    "phpunit/phpunit": "^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "HughCube\\Laravel\\Migrations\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit --verbose --coverage-clover=coverage.clover",
    "phpstan": "phpstan analyse -vvv",
    "check-style": "phpcs -p --standard=PSR2 src/ -v",
    "fix-style": "phpcbf -p --standard=PSR2 src/ -v"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
